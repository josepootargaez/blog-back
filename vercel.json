{
    "version": 2,
    "builds": [
      {
        "src": "public/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
      ]
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }